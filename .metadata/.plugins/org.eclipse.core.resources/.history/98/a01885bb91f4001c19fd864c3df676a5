package application;

public class Node {

	int value;
	Node right;
	Node left;

	public Node(int value, Node right, Node left) {

		this.value = value;
		this.right = right;
		this.left = left;
	}

	public int getValue() {
		return value;
	}

	public Node getRight() {
		return right;
	}

	public Node getLeft() {
		return left;
	}

	public void setRight(Node right) {
		this.right = right;
	}

	public void setLeft(Node left) {
		this.left = left;
	}

	public void setValue(int value) {
		this.value = value;
	}

}